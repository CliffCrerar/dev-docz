  },
  "homepage": "https://github.com/microapps/Dinosaur-Mask#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-preset-es2015": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "connect-pause": "^0.1.0",
    "debug": "^2.6.8",
    "debut": "^0.0.14",
    "json-server": "^0.12.1",
    "nodemon": "^1.12.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "eslint": "4.11.0",
    "eslint-config-microapps-react": "^1.0.0",
    "eslint-config-prettier": "2.7.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "prettier": "1.8.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "better-npm-run start",
    "start:local": "better-npm-run start:local"
  },
  "betterScripts": {
    "start": {
      "command": "babel-node index.js",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "start:local": {
      "command": "nodemon --exec babel-node index.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    }
  }
}
